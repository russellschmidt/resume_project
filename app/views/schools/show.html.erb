<div class="page-header">
  <h1>Inspect a School</h1>
</div>
<div class="well well-small">
  <p>Review this specific educational experience.</p>
</div>
<%= render 'nav' %>

<dl class="dl-horizontal">
  <dt>School</dt>
  <dd><%= @school.school_name %></dd>
  <dt>Degree</dt>
  <dd><%= @school.degree %></dd>
  <dt>Major</dt>
  <dd><%= @school.major %></dd>
  <dt>Description</dt>
  <dd><%= @school.description %></dd>
  <dt>Start Date</dt>
  <dd><%= @school.start_date %></dd>
  <dt>End Date</dt>
  <dd><%= @school.end_date %></dd>
</dl>