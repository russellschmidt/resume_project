<div class="page-header">
  <h1>Inspect a Job</h1>
</div>
<div class="well well-small">
  <p>Review this specific work experience.</p>
</div>
<%= render 'nav' %>

<dl class="dl-horizontal">
  <dt>Company</dt>
  <dd><%= @job.company %></dd>
  <dt>Title</dt>
  <dd><%= @job.title %></dd>
  <dt>Description</dt>
  <dd><%= @job.description %></dd>
  <dt>Start Date</dt>
  <dd><%= @job.start_date %></dd>
  <dt>End Date</dt>
  <dd><%= @job.end_date %></dd>
</dl>

<%= render 'signout' %>